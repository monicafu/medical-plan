        "objectId": {"type": "string"},
        "objectType": {"type": "string"},
        "name": {"type": "string"}
      },
      "required": [
        "name",
        "objectId",
        "objectType"
      ],
      "title": "linkedService"
    },
    "planCostShares": {
      "type": "object",
      "properties": {
        "deductible": {"type": "integer"},
        "_org": {"type": "string"},
        "copay": {"type": "integer"},
        "objectId": {"type": "string"},
        "objectType": {"type": "string"}
      },
      "required": [
        "copay",
        "deductible",
        "objectId",
        "objectType"
      ],
      "title": "planCostShares"
    }
  }
}